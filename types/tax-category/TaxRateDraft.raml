#%RAML 1.0 DataType
(package): TaxCategory
type: object
displayName: TaxRateDraft
properties:
  name:
    type: string
    description: ''
  amount?:
    type: number
    minimum: 0
    maximum: 1
    description: |-
      Percentage in the range of [0..1].
      Must be supplied if no `subRates` are specified.
      If `subRates` are specified
      then the `amount` can be omitted or it must be the sum of the amounts of all `subRates`.
  includedInPrice:
    type: boolean
    description: ''
  country:
    type: CountryCode
    description: A two-digit country code as per {% include external/country_code.html
      %}.
  state?:
    type: string
    description: The state in the country
  subRates?:
    type: SubRate[]
    description: |-
      For countries (e.g.
      the US) where the total tax is a combination of multiple taxes (e.g.
      state and local taxes).
